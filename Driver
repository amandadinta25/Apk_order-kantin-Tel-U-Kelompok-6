package entitas_kelompok_6;

public class driver_kelompok_6 {
    private String iddriver;
    private String namadriver;
    private String nohpdriver;
    
    //membuat constructor untuk driver
    public driver_kelompok_6(String Iddriver, String Namadriver, String Nohpdriver){
        iddriver = Iddriver;
        namadriver = Namadriver;
        nohpdriver = Nohpdriver;
    }
    
    //membuat setter untuk id_driver
    public void setIddriver(String Id_driver){
        iddriver = Id_driver;
    }
    
    //membuat setter untuk nama_driver
    public void setNamadriver(String Nama_driver){
        namadriver = Nama_driver;
    }
    
    //membuat setter untuk no_hp_driver
    public void setNohpdriver(String Nohp_driver){
        nohpdriver = Nohp_driver;
    }
    
    //membuat getter untuk id_driver
    public String getIddriver(){
        return iddriver;
    }
    
    //membuat getter untuk nama_driver
    public String getNamadriver(){
        return namadriver;
    }
    
    //membuat getter untuk no_hp_driver
    public String getNohpdriver(){
        return nohpdriver;
    }
}
